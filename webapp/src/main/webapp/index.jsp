	<script language="javascript" type="text/javascript">
function validateContact(){
	var nameformat = /^[A-Za-z\s]+$/;
	var nameformat1 = /^[A-Za-z\s]+$/;
	var mailformat = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/;	
	var sNumber = /^(?:(?:\+|0{0,2})91(\s*[\-]\s*)?|[0]?)?[789]\d{9}$/;
	
	if(document.contact.text1_smallName.value == ""){
		alert("First Name should not be empty.");
		document.contact.text1_smallName.focus();
		return false;
	}
    match = nameformat.test(document.contact.text1_smallName.value);
    if(!match){
    alert("Please provide a valid First Name.");
    document.contact.text1_smallName.focus();
    return false;
	}
	if(document.contact.txtlast.value == ""){
		alert("Last Name should not be empty.");
		document.contact.txtlast.focus();
		return false;
	}
    match = nameformat1.test(document.contact.txtlast.value);
    if(!match){
    alert("Please provide a valid Last Name.");
    document.contact.txtlast.focus();
    return false;
	}
	if(document.contact.txtjtitle.value == ""){
		alert("Job Title should not be empty.");
		document.contact.txtjtitle.focus();
		return false;
	}
	if(document.contact.txtcompany.value == ""){
		alert("Company Name should not be empty.");
		document.contact.txtcompany.focus();
		return false;
	}
	if(document.contact.text1_smallEmail.value == ""){
    	alert("Please enter email address.");
		document.contact.text1_smallEmail.focus();
		return false;
	}
    match = mailformat.test(document.contact.text1_smallEmail.value);
    if(!match){
    alert("Please provide a valid Email.");
    document.contact.text1_smallEmail.focus();
    return false;
    }
	if(document.contact.text1_smallPhone.value == ""){
        alert("Please enter Phone Number.");
		document.contact.text1_smallPhone.focus();
		return false;
	}
}
</script>
